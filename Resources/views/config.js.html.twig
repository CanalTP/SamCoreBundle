<script>
var require = {
    baseUrl: '{{app.request.basePath}}/',
    urlArgs: '{{ assets_version }}',
    paths: {
        // libs
        jquery:                         'components/jquery/jquery',
        jquery_ui_sortable:             'components/jquery-ui/ui/minified/jquery.ui.sortable.min',
        jquery_ui_core:                 'components/jquery-ui/ui/minified/jquery.ui.core.min',
        jquery_ui_mouse:                'components/jquery-ui/ui/minified/jquery.ui.mouse.min',
        jquery_ui_widget:               'components/jquery-ui/ui/minified/jquery.ui.widget.min',
        jquery_ui_position:             'components/jquery-ui/ui/minified/jquery.ui.position.min',
        jquery_ui_menu:                 'components/jquery-ui/ui/minified/jquery.ui.menu.min',
        jquery_ui_autocomplete:         'components/jquery-ui/ui/minified/jquery.ui.autocomplete.min',
        bootstrap:                      'js/bootstrap',
        'bootstrap/datepicker':         'components/bootstrap-datepicker/bootstrap-datepicker-built',
        'bootstrap/datepicker/fr':      'bundles/canaltpsamcore/js/bootstrap-datepicker.fr',
        // routes
        fosjsrouting:                   'bundles/fosjsrouting/js/router',
        sf_routes:                      '{{ path("fos_js_routing_js", {"callback": "fos.Router.setData"}) }}',
        common:                         'bundles/canaltpsamcore/js/common',
        // translations
        translator:                     'bundles/bazingajstranslation/js/translator.min',
        'translations/default':         'js/translations/default/{{ app.request.locale }}',
        'translations/messages':        'js/translations/messages/{{ app.request.locale }}',
        // base
        base:                           'bundles/canaltpsamcore/js/base'
    },
    shim: {
        sf_routes: {
            deps: ['fosjsrouting']
        },
        translator: {
            exports: "Translator"
        },
        "translations/default":{
            deps: ["translator"],
            exports: "Translator"
        },
        "translations/messages":{
            deps: ["translator"],
            exports: "Translator"
        },
        bootstrap: {
            deps: ['jquery']
        },
        'bootstrap/datepicker': {
            deps: ['bootstrap']
        },
        'bootstrap/datepicker/fr': {
            deps: ['bootstrap/datepicker']
        },
        jquery_ui_core: {
            deps: ['jquery'],
            exports: "jQuery"
        },
        jquery_ui_mouse: {
            deps: ['jquery', 'jquery_ui_widget'],
            exports: "jQuery"
        },
        jquery_ui_widget: {
            deps: ['jquery'],
            exports: "jQuery"
        },
        jquery_ui_sortable: {
            deps: ['jquery', 'jquery_ui_core','jquery_ui_mouse','jquery_ui_widget'],
            exports: "jQuery"
        },
        jquery_ui_position: {
            deps: ['jquery'],
            exports: "jQuery"
        },
        jquery_ui_menu: {
            deps: ['jquery_ui_core','jquery_ui_widget', 'jquery_ui_position'],
            exports: "jQuery"
        },
        jquery_ui_autocomplete: {
            deps: ['jquery_ui_core','jquery_ui_widget','jquery_ui_position','jquery_ui_menu'],
            exports: "jQuery"
        }
    },
    deps: ["jquery", 'bootstrap', "sf_routes", "translator", "common"]
};
