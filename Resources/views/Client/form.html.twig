{% extends '::base.html.twig' %}

{% block left_menu -%}
    {% include 'CanalTPSamCoreBundle:Client:left_menu.html.twig' %}
{% endblock %}

{% block main_content %}

    <h1>{{ 'client.edit.title'|trans }}</h1>
    <hr/>

    {{ form_start(form, { 'style': 'horizontal' }) }}
        {{ form_row(form.name) }}
        
        {{ form_row(form.navitiaToken) }}
        
        {{ form_widget(form._token) }}
        <button type="submit" class="btn btn-default">{{ 'form.sauvegarder'|trans }}</button>
                            
    
    {{ form_end(form) }}
    <hr/>

{% endblock %}
